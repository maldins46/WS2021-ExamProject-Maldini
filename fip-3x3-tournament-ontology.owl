    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/date">
        <rdfs:comment xml:lang="en">A point or period of time associated with an event in the lifecycle of the resource.</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#awayPoints -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#awayPoints">
