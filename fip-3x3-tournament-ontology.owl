    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/ontology/location -->

    <owl:ObjectProperty rdf:about="http://dbpedia.org/ontology/location"/>
    


    <!-- http://www.bbc.co.uk/ontologies/sport/awayCompetitor -->

    <rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/sport/awayCompetitor">
        <rdfs:subPropertyOf rdf:resource="http://www.bbc.co.uk/ontologies/sport/hasCompetitor"/>
    </rdf:Description>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasEdition -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasEdition">
        <rdfs:subPropertyOf rdf:resource="http://www.bbc.co.uk/ontologies/sport/recurringCompetition"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isEditionOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#SummerTournament"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Edition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasMVP -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasMVP">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isMVPonMatch"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Match"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Player"/>
        <rdfs:comment xml:lang="en">The MVP is the player that scored more points through the match.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasPlayer -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasPlayer">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isPlayerOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Player"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Team"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasStaffMember -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasStaffMember">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#staffMemberOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Edition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#StaffMember"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasTier -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasTier">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasTier"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Edition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Tier"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasVenue -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasVenue">
        <rdfs:subPropertyOf rdf:resource="http://dbpedia.org/ontology/location"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isEditionOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isEditionOf"/>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isMVPonMatch -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isMVPonMatch"/>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isPlayerOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isPlayerOf"/>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isTierOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#isTierOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Tier"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Edition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#staffMemberOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#staffMemberOf"/>
    


