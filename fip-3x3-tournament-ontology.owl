    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#editionYear -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#editionYear">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Edition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#fromDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#fromDate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/date"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">A starting point of a period of time associated with an event in the lifecycle of the resource.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#homePoints -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#homePoints">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#matchPoints"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#matchPoints -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#matchPoints"/>
    


