    <!-- http://dbpedia.org/ontology/Place -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Place"/>
    


    <!-- http://www.bbc.co.uk/ontologies/sport/GroupCompetition -->

    <rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/sport/GroupCompetition">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bbc.co.uk/ontologies/sport/hasRound"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#GroupRound"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>
    


    <!-- http://www.bbc.co.uk/ontologies/sport/Round -->

    <rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/sport/Round">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bbc.co.uk/ontologies/sport/roundType"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.bbc.co.uk/ontologies/sport/RoundType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Edition -->

    <owl:Class rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Edition">
        <rdfs:subClassOf rdf:resource="http://www.bbc.co.uk/ontologies/sport/MultiStageCompetition"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Fip3x3DomainEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasStaffMember"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#StaffMember"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasVenue"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Playground"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bbc.co.uk/ontologies/sport/eventGender"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.bbc.co.uk/ontologies/sport/EventGender"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasTier"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Tier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#fromDate"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#toDate"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#year"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A single istance of a FIP Summer Tournament. Each tournament should have at least one edition, and it could have more edition on each year (for example, for male and female).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Fip3x3DomainEntity -->

    <owl:Class rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Fip3x3DomainEntity"/>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#GroupRound -->

    <owl:Class rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#GroupRound">
        <rdfs:subClassOf rdf:resource="http://www.bbc.co.uk/ontologies/sport/Round"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Fip3x3DomainEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bbc.co.uk/ontologies/sport/hasCompetitor"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Team"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bbc.co.uk/ontologies/sport/hasMatch"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Match"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://xmlns.com/foaf/0.1/nick"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The Group Rounds are the first rounds of the tournament. It is organized as a collection of teams that battle each other. The top N competitors from each Group go to the Knockout Rounds.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#KnockoutRound -->

    <owl:Class rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#KnockoutRound">
        <rdfs:subClassOf rdf:resource="http://www.bbc.co.uk/ontologies/sport/Round"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Fip3x3DomainEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bbc.co.uk/ontologies/sport/hasCompetitor"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Team"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bbc.co.uk/ontologies/sport/hasMatch"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Match"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The Knockout Rounds involves a decreasing number of teams battling each other, until the Final Round that decides the winner team of the tounrmanent.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Match -->

    <owl:Class rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Match">
        <rdfs:subClassOf rdf:resource="http://www.bbc.co.uk/ontologies/sport/Match"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Fip3x3DomainEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bbc.co.uk/ontologies/sport/awayCompetitor"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Match"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bbc.co.uk/ontologies/sport/homeCompetitor"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Match"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#hasMVP"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#Player"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#onDate"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#awayPoints"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
