    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#numberOfCourts">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#onDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#onDate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/date"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">A precise point on of time associated with an event in the lifecycle of the resource.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#toDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/riccardomaldini/ontologies/fip-3x3-ontology#toDate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/date"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">An ending point of a period of time associated with an event in the lifecycle of the resource.</rdfs:comment>
    </owl:DatatypeProperty>
    


